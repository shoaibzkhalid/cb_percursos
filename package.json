{
  "name": "cb_percursos",
  "version": "1.6.1",
  "private": true,
  "scripts": {
    "ios": "npx react-native run-ios --simulator=\"iPhone 14\"",
    "ipad": "npx react-native run-ios --simulator=\"iPad mini (6th generation)\"",
    "android": "npx react-native run-android",
    "start": "react-native start",
    "lint": "eslint .",
    "pre:build": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle",
    "post:build": "cd android && ./gradlew clean && ./gradlew assembleRelease",
    "clean": "cd ios && rm -rf Pods Podfile.lock && pod install && cd .. && yarn ios",
    "prod": "npx react-native run-android --variant=release",
    "prod-logs": "adb logcat",
    "build": "yarn pre:build && yarn post:build",
    "cache-clean": "react-native start --reset-cache",
    "dist": "firebase appdistribution:distribute android/app/build/outputs/apk/release/app-armeabi-v7a-release.apk --app 1:334901946661:android:54f4a9c8cd9218967cd2cc --groups team --release-notes-file release-notes.txt",
    "pub": "git push && git push cat master && yarn dist",
    "bundle": "cd android && ./gradlew clean && ./gradlew bundleRelease",
    "push": "git push cat master && yarn build && yarn dist",
    "list-ios-devices": "xcrun simctl list devices",
    "getip": "adb shell \"ifconfig | grep -A 1 wlan0 | tail -n 1 | cut -f2 -d: | cut -f1 -d' '\""
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^6.4.0",
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/stack": "^6.3.2",
    "@reduxjs/toolkit": "^1.8.6",
    "dayjs": "^1.11.6",
    "i18n-js": "^4.1.1",
    "lodash": "^4.17.21",
    "native-base": "^3.4.19",
    "react": "18.1.0",
    "react-native": "0.70.4",
    "react-native-chart-kit": "^6.12.0",
    "react-native-gesture-handler": "^2.8.0",
    "react-native-get-location": "^2.2.1",
    "react-native-maps": "^1.3.2",
    "react-native-maps-directions": "^1.9.0",
    "react-native-reanimated": "^2.11.0",
    "react-native-safe-area-context": "^4.4.1",
    "react-native-screens": "^3.18.2",
    "react-native-svg": "^13.4.0",
    "react-native-svg-transformer": "^1.0.0",
    "react-redux": "^8.0.4",
    "redux": "^4.2.0",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "0.72.3",
    "react-test-renderer": "18.1.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
